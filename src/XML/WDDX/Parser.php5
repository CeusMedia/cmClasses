 */
class XML_WDDX_Parser
{
	/**
	 *	Deserializes a wddx packet.
	 *	@access		public
	 *	@static
	 *	@return		mixed
	 */
	public static function parse( $packet )
	{
		return wddx_deserialize( $packet );
	}
}
